 arxivid = {1708.04897},
 author = {Bazavov, A. and Ding, H.-T. and Hegde, P. and Kaczmarek, O. and Karsch, F. and Laermann, E. and Mukherjee, Swagato and Ohno, H. and Petreczky, P. and Rinaldi, E. and Sandmeyer, H. and Schmidt, C. and Schroeder, Chris and Sharma, S. and Soeldner, W. and Soltz, R. A. and Steinbrecher, P. and Vranas, P. M.},
 doi = {10.1103/PhysRevD.96.074510},
