 file = {:Users/enrythebest/Mendeley Desktop/Berkowitz et al. - 2016 - Precision lattice test of the gaugegravity duality at large N.pdf:pdf},
 issn = {2470-0010},
 journal = {Physical Review D},
 keywords = {duality,gauge,gravity,lattice,monte-carlo},
 mendeley-tags = {duality,gauge,gravity,lattice,monte-carlo},
 month = {nov},
 number = {9},
 pages = {094501},
 title = {Precision lattice test of the gauge/gravity duality at large N},
 url = {http://arxiv.org/abs/1606.04951 http://link.aps.org/doi/10.1103/PhysRevD.94.094501},
